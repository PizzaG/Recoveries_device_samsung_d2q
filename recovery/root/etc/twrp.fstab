## PizzaG Custom fstab


/system		     ext4   /dev/block/bootdevice/by-name/system         flags=backup=0;display="System"
/system_image    emmc	/dev/block/bootdevice/by-name/system         flags=backup=1;flashimg
/vendor		     ext4	/dev/block/bootdevice/by-name/vendor         flags=backup=0;display="Vendor"
/vendor_image	 emmc   /dev/block/bootdevice/by-name/vendor         flags=backup=1;flashimg
/product	     ext4	/dev/block/bootdevice/by-name/product        flags=backup=0;display="Product"
/product_image   emmc   /dev/block/bootdevice/by-name/product	     flags=backup=1;flashimg

/boot		     emmc	/dev/block/bootdevice/by-name/boot
/recovery	     emmc	/dev/block/bootdevice/by-name/recovery
/data		     ext4	/dev/block/bootdevice/by-name/userdata       flags=wait;encryptable=footer;length=-16384
/cache		     ext4	/dev/block/bootdevice/by-name/cache		     flags=backup=1;display="Cache"
/efs	         ext4	/dev/block/bootdevice/by-name/sec_efs	     
/firmware	     emmc	/dev/block/bootdevice/by-name/apnhlos	     
/modem		     emmc	/dev/block/bootdevice/by-name/modem		     
/dsp		     emmc	/dev/block/bootdevice/by-name/dsp
/misc		     emmc	/dev/block/bootdevice/by-name/misc
/keydata	     ext4	/dev/block/bootdevice/by-name/keydata
/keyrefuge	     ext4	/dev/block/bootdevice/by-name/keyrefuge
/dtbo		     emmc	/dev/block/bootdevice/by-name/dtbo			 

# Removable storage
/external_sd	 auto	/dev/block/mmcblk0p1	/dev/block/mmcblk0   flags=display="Micro-SDcard";storage;wipeingui;removable
/usb-otg	     auto	/dev/block/sde1	/dev/block/sde1				 flags=display="USB-OTG";storage;wipeingui;removable
